<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-1.0.xsd">

	<start-state idref="execute"/>
	
	<action-state id="execute">
		<action bean="messageSourceAwareAction"/>
		<transition on="success" to="finish"/>
	</action-state>
	
	<end-state id="finish"/>
	
	<import resource="messageSourceAwareContext.xml" />

</flow>